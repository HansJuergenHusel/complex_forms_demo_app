<div class="category"> 
<% prefix = "project[category_attributes][]" %> 
<% fields_for prefix, category do |category_form| -%> 
<p> 
  <%= category_form.check_box :id, {:checked => checked.include?(category)}, category.id %> 
  <label for="project_category_attributes_<%=  category.id  %>_id"><%= category.name %></label>
</p> 
<% end -%> 
</div> 
