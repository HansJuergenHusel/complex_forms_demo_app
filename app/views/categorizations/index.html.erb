<h1>Listing categorizations</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Category</th>
  </tr>

<% for categorization in @categorizations %>
  <tr>
    <td><%=h assignment.project.name %></td>
    <td><%=h assignment.category.name %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New categorization', new_categorization_path %>
