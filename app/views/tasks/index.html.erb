<h1>Listing tasks</h1>

<table>
  <tr>
    <th>Project</th>
    <th>Name</th>
    <th>Completed</th>
  </tr>

<% for task in @tasks %>
  <tr>
    <td><%=h task.project.name %></td>
    <td><%=h task.name %></td>
    <td><%=h task.complete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New task', new_task_path %>
